{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "import", "jsx-a11y"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "__ENV__": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "arrow-parens": 1,
    "comma-dangle": ["error", "never"],
    "react/forbid-prop-types": [1, { "forbid": ["any"]}]
  }
}
